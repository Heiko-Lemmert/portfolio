<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /
  
  # Wenn die angeforderte Datei oder der Ordner existiert, direkt ausliefern
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  
  # Andernfalls alles auf index.html umleiten
  RewriteRule ^.*$ /index.html [L]
</IfModule>
